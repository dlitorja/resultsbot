{
  "name": "resultsbot",
  "version": "1.0.0",
  "description": "A fun Discord bot with job posting features, birthday tracking, and some jokes",
  "main": "dist/start.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/start.ts",
    "build": "tsc",
    "start": "node dist/start.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "register-commands": "tsx src/bot/registerCommands.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "typescript",
    "discord.js",
    "jobs",
    "birthdays"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^7.120.4",
    "@supabase/supabase-js": "^2.38.4",
    "axios": "^1.6.2",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "luxon": "^3.4.4",
    "node-cron": "^3.0.3",
    "p-queue": "^8.0.1",
    "pino": "^10.1.0",
    "pino-pretty": "^10.3.0",
    "prom-client": "^15.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/luxon": "^3.3.7",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitest/ui": "^4.0.8",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.46.4",
    "vitest": "^4.0.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
